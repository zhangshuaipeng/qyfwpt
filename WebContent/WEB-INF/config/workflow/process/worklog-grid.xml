      <width>48</width>
      <length>22</length>
      <precision>0</precision>
    </model.grid.ColumnModel>
    <model.grid.ColumnModel>
      <code>nodename</code>
      <title>环节</title>
      <showtype>TEXT</showtype>
      <isnotnull>Y</isnotnull>
      <datatype>STRING</datatype>
      <align>center</align>
      <width>64</width>
