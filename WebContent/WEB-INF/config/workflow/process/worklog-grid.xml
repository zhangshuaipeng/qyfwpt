      <dicttype>L</dicttype>
      <dictid>assigntype</dictid>
      <length>0</length>
      <precision>0</precision>
    </model.grid.ColumnModel>
    <model.grid.ColumnModel>
      <code>starttime</code>
      <title>开始时间</title>
