      <title>流程实例号</title>
      <showtype>HIDDEN</showtype>
      <length>0</length>
      <precision>0</precision>
    </model.grid.ColumnModel>
  </columns>
  <opts>
    <model.grid.QueryOptModel>
      <id>instanceid</id>
      <title>流程实例号</title>
      <field>instanceid</field>
      <type>LINK</type>
      <option>=</option>
      <required>Y</required>
      <display>HIDDEN</display>
      <dictid>instanceid</dictid>
    </model.grid.QueryOptModel>
    <model.grid.QueryOptModel>
      <id>order</id>
      <title>排序</title>
      <field>starttime</field>
      <type>FILTER</type>
      <option>ASC</option>
    </model.grid.QueryOptModel>
  </opts>
  <actions>
    <model.view.ActionModel>
      <code>open</code>
      <name>打开表单</name>
      <type>POPUP</type>
      <showtype>LINK</showtype>
