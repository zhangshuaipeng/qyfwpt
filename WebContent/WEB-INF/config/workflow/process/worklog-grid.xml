      <datatype>STRING</datatype>
      <align>center</align>
      <width>64</width>
      <length>19</length>
      <precision>0</precision>
    </model.grid.ColumnModel>
    <model.grid.ColumnModel>
      <code>comments</code>
      <title>意见</title>
      <showtype>TEXT</showtype>
      <isnotnull>N</isnotnull>
      <datatype>STRING</datatype>
      <align>center</align>
      <width>128</width>
      <length>1024</length>
      <precision>0</precision>
    </model.grid.ColumnModel>
