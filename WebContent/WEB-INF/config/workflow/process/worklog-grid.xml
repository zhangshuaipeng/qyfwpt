        </model.grid.StyleModel>
        <model.grid.StyleModel>
          <code>actorname</code>
          <name>办理人</name>
          <newline>Y</newline>
          <align>left</align>
        </model.grid.StyleModel>
        <model.grid.StyleModel>
          <code>assignername</code>
          <name>递交人名</name>
          <newline>Y</newline>
          <align>left</align>
        </model.grid.StyleModel>
        <model.grid.StyleModel>
          <code>submitmode</code>
          <name>处理方式</name>
          <newline>Y</newline>
          <align>left</align>
        </model.grid.StyleModel>
        <model.grid.StyleModel>
          <code>starttime</code>
          <name>开始时间</name>
          <newline>Y</newline>
          <align>left</align>
        </model.grid.StyleModel>
        <model.grid.StyleModel>
          <code>comments</code>
          <name>处理意见</name>
          <newline>Y</newline>
          <align>left</align>
        </model.grid.StyleModel>
