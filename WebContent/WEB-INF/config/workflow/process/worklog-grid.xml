      <display>ICONTEXT</display>
      <args>wid:$WID</args>
      <width>1024</width>
      <height>600</height>
      <operand>ROW</operand>
      <icon>db.gif</icon>
      <target>_self</target>
    </model.view.ActionModel>
    <model.view.ActionModel>
      <code>exit</code>
      <name>返回</name>
      <type>JAVASCRIPT</type>
      <showtype>BUTTON</showtype>
      <url>returnForm();</url>
      <display>ICONTEXT</display>
      <operand>FREE</operand>
      <icon>exit.gif</icon>
      <target>_self</target>
    </model.view.ActionModel>
