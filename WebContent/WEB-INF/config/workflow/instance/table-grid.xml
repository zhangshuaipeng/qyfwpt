<?xml version="1.0" encoding="UTF-8"?>
<model.grid.GridModel>
  <system>workflow</system>
  <module>instance</module>
  <code>table</code>
  <name>列表</name>
  <rowsperpage>0</rowsperpage>
  <box>none</box>
  <index>Y</index>
  <pagecontrol>Y</pagecontrol>
  <state>readonly</state>
  <columns>
    <model.grid.ColumnModel>
      <code>instanceid</code>
      <title>实例号</title>
      <width>48</width>
      <align>center</align>
      <datatype>INTEGER</datatype>
      <showtype>TEXT</showtype>
      <length>22</length>
      <precision>0</precision>
      <isnotnull>Y</isnotnull>
    </model.grid.ColumnModel>
    <model.grid.ColumnModel>
      <code>title</code>
      <title>工作名称</title>
      <width>128</width>
      <align>center</align>
      <datatype>STRING</datatype>
      <showtype>TEXT</showtype>
      <actionid>log</actionid>
      <length>255</length>
      <precision>0</precision>
      <isnotnull>N</isnotnull>
    </model.grid.ColumnModel>
    <model.grid.ColumnModel>
      <code>sponsorname</code>
      <title>发起人</title>
      <width>64</width>
      <align>center</align>
      <datatype>STRING</datatype>
      <showtype>TEXT</showtype>
      <length>128</length>
      <precision>0</precision>
      <isnotnull>Y</isnotnull>
    </model.grid.ColumnModel>
    <model.grid.ColumnModel>
      <code>status</code>
      <title>状态</title>
      <width>32</width>
      <align>center</align>
      <datatype>STRING</datatype>
      <showtype>COMBOX</showtype>
      <dicttype>L</dicttype>
      <dictid>flowstatus</dictid>
      <length>1</length>
      <precision>0</precision>
      <isnotnull>Y</isnotnull>
    </model.grid.ColumnModel>
    <model.grid.ColumnModel>
      <code>curnodename</code>
      <title>当前环节名</title>
      <width>80</width>
      <align>center</align>
      <datatype>STRING</datatype>
      <showtype>TEXT</showtype>
      <length>64</length>
      <precision>0</precision>
      <isnotnull>Y</isnotnull>
    </model.grid.ColumnModel>
    <model.grid.ColumnModel>
      <code>starttime</code>
      <title>开始时间</title>
      <width>64</width>
      <align>center</align>
      <datatype>STRING</datatype>
      <showtype>TEXT</showtype>
      <length>19</length>
      <precision>0</precision>
      <isnotnull>Y</isnotnull>
    </model.grid.ColumnModel>
    <model.grid.ColumnModel>
      <code>endtime</code>
      <title>结束时间</title>
      <width>64</width>
      <align>center</align>
      <datatype>STRING</datatype>
      <showtype>TEXT</showtype>
      <length>19</length>
      <precision>0</precision>
      <isnotnull>N</isnotnull>
    </model.grid.ColumnModel>
    <model.grid.ColumnModel>
      <code>floworgcode</code>
      <title>流程的组织号</title>
      <width>96</width>
      <align>center</align>
      <datatype>STRING</datatype>
      <showtype>HIDDEN</showtype>
      <length>32</length>
      <precision>0</precision>
      <isnotnull>Y</isnotnull>
    </model.grid.ColumnModel>
    <model.grid.ColumnModel>
      <code>flowcode</code>
      <title>流程号</title>
      <width>48</width>
      <align>center</align>
      <datatype>STRING</datatype>
      <showtype>HIDDEN</showtype>
      <length>32</length>
      <precision>0</precision>
      <isnotnull>Y</isnotnull>
    </model.grid.ColumnModel>
    <model.grid.ColumnModel>
      <code>currentwid</code>
      <title>当前环节</title>
      <width>64</width>
      <align>center</align>
      <datatype>INTEGER</datatype>
      <showtype>HIDDEN</showtype>
      <length>22</length>
      <precision>0</precision>
      <isnotnull>Y</isnotnull>
    </model.grid.ColumnModel>
    <model.grid.ColumnModel>
      <code>abort</code>
      <title>终止</title>
      <width>32</width>
      <align>center</align>
      <showtype>IMAGE</showtype>
      <actionid>abort</actionid>
      <dictid>del.png</dictid>
      <length>0</length>
      <precision>0</precision>
    </model.grid.ColumnModel>
    <model.grid.ColumnModel>
      <code>urged</code>
      <title>催办</title>
      <width>32</width>
      <align>center</align>
      <showtype>IMAGE</showtype>
      <actionid>urged</actionid>
      <dictid>msgsend.gif</dictid>
      <length>0</length>
      <precision>0</precision>
    </model.grid.ColumnModel>
    <model.grid.ColumnModel>
      <code>goto</code>
      <title>跳转</title>
      <width>32</width>
      <align>center</align>
      <showtype>IMAGE</showtype>
      <actionid>goto</actionid>
      <dictid>return.gif</dictid>
      <length>0</length>
      <precision>0</precision>
    </model.grid.ColumnModel>
  </columns>
  <opts>
    <model.grid.QueryOptModel>
      <id>orgcode</id>
      <title>组织号</title>
      <field>floworgcode</field>
      <type>LINK</type>
      <option>=</option>
      <required>Y</required>
      <value>{ORGCODE}</value>
      <display>HIDDEN</display>
    </model.grid.QueryOptModel>
    <model.grid.QueryOptModel>
      <id>flowcode</id>
      <title>流程号</title>
      <field>flowcode</field>
      <type>LINK</type>
      <option>=</option>
      <required>Y</required>
      <display>HIDDEN</display>
      <dictid>flowcode</dictid>
    </model.grid.QueryOptModel>
    <model.grid.QueryOptModel>
      <id>title</id>
      <title>工作名称</title>
      <field>title</field>
      <type>FILTER</type>
      <option>LIKE</option>
      <display>TEXT</display>
    </model.grid.QueryOptModel>
    <model.grid.QueryOptModel>
      <id>begin</id>
      <title>查询起始时间</title>
      <field>starttime</field>
      <type>FILTER</type>
      <option>&gt;</option>
      <display>POPUP</display>
      <dictid>DATE</dictid>
    </model.grid.QueryOptModel>
    <model.grid.QueryOptModel>
      <id>end</id>
      <title>查询结束时间</title>
      <field>starttime</field>
      <type>FILTER</type>
      <option>&lt;</option>
      <display>POPUP</display>
      <dictid>DATE</dictid>
    </model.grid.QueryOptModel>
    <model.grid.QueryOptModel>
      <id>status</id>
      <title>流程状态</title>
      <field>status</field>
      <type>FILTER</type>
      <option>=</option>
      <display>COMBOX</display>
      <dictid>flowstatus</dictid>
    </model.grid.QueryOptModel>
    <model.grid.QueryOptModel>
      <id>timeorder</id>
      <title>发起时间</title>
      <field>starttime</field>
      <type>LINK</type>
      <option>DESC</option>
      <required>Y</required>
    </model.grid.QueryOptModel>
  </opts>
  <actions>
    <model.view.ActionModel>
      <code>abort</code>
      <name>终止流程</name>
      <type>ROW</type>
      <showtype>BUTTON</showtype>
      <href>abort</href>
      <display>ICONTEXT</display>
      <args>currentwid:wid</args>
      <operand>ROW</operand>
      <icon>del.png</icon>
      <target>_self</target>
    </model.view.ActionModel>
    <model.view.ActionModel>
      <code>urged</code>
      <name>催办</name>
      <type>AJAX</type>
      <showtype>BUTTON</showtype>
      <href>urged</href>
      <display>ICONTEXT</display>
      <args>currentwid:wid</args>
      <operand>ROW</operand>
      <icon>msgsend.gif</icon>
      <target>_self</target>
    </model.view.ActionModel>
    <model.view.ActionModel>
      <code>goto</code>
      <name>跳转</name>
      <type>POPUP</type>
      <showtype>BUTTON</showtype>
      <href>URL</href>
      <url>workflow/process.cmd?$ACTION=route&amp;mode=goto</url>
      <display>ICONTEXT</display>
      <args>currentwid:wid</args>
      <width>600</width>
      <height>400</height>
      <operand>ROW</operand>
      <icon>exit.gif</icon>
      <target>_self</target>
    </model.view.ActionModel>
    <model.view.ActionModel>
      <code>log</code>
      <name>流转过程</name>
      <type>POPUP</type>
      <showtype>LINK</showtype>
      <href>URL</href>
      <url>frame.jsp?system=workflow&amp;module=process&amp;name=log</url>
      <display>ICONTEXT</display>
      <args>instanceid</args>
      <width>1024</width>
      <height>640</height>
      <operand>ROW</operand>
      <icon>teamadd.gif</icon>
      <target>_self</target>
    </model.view.ActionModel>
  </actions>
  <rowdisplay>
    <model.grid.DisplayModel>
      <id>status</id>
      <col>status</col>
      <value>F</value>
      <hidecols>abort,urged,goto</hidecols>
    </model.grid.DisplayModel>
    <model.grid.DisplayModel>
      <id>status1</id>
      <col>status</col>
      <value>A</value>
      <hidecols>abort,urged,goto</hidecols>
    </model.grid.DisplayModel>
    <model.grid.DisplayModel>
      <id>status2</id>
      <col>status</col>
      <value>D</value>
      <hidecols>abort,urged,goto</hidecols>
    </model.grid.DisplayModel>
  </rowdisplay>
  <query>
    <business>retrieve.eap_instance</business>
  </query>
</model.grid.GridModel>