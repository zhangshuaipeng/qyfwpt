      <style>space</style>
      <jsfile>&lt;script&gt;&#xd;
&#xd;
function selectUser() {&#xd;
   if (!isSelected()) {&#xd;
       alert(&quot;请选择目录！&quot;);&#xd;
       return;&#xd;
   }&#xd;
   var v = getSelectedValues();&#xd;
   if (v.indexOf(&quot;,&quot;)!=-1) {   // 多选&#xd;
      var str = v.split(&quot;,&quot;);&#xd;
      var code = &quot;&quot;;&#xd;
      var name = &quot;&quot;;&#xd;
      for (var i=0;i&lt;str.length;i++) {&#xd;
          if (code!=&quot;&quot;) {&#xd;
              code += &quot;,&quot;;&#xd;
              name += &quot;,&quot;;&#xd;
          }&#xd;
          code += str[i].substring(0,str[i].indexOf(&quot;|&quot;));&#xd;
          name += str[i].substring(str[i].indexOf(&quot;|&quot;) + 1);&#xd;
      }&#xd;
      v = code + &quot;|&quot; + name;&#xd;
   }&#xd;
   top.window.submitDialog(window,v);     &#xd;
}&#xd;
&lt;/script&gt;</jsfile>
