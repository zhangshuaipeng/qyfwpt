<?xml version="1.0" encoding="UTF-8"?>
<model.grid.GridModel>
  <system>ou</system>
  <module>group</module>
  <code>grpusers</code>
  <name>群组用户</name>
  <rowsperpage>20</rowsperpage>
  <box>checkbox</box>
  <index>Y</index>
  <pagecontrol>Y</pagecontrol>
  <state>readonly</state>
  <columns>
    <model.grid.ColumnModel>
      <code>pk</code>
      <title>记录号</title>
      <width>48</width>
      <align>center</align>
      <datatype>INTEGER</datatype>
      <showtype>HIDDEN</showtype>
      <length>22</length>
      <precision>0</precision>
      <isnotnull>Y</isnotnull>
    </model.grid.ColumnModel>
    <model.grid.ColumnModel>
      <code>orgcode</code>
      <title>组织号</title>
      <width>48</width>
      <align>center</align>
      <datatype>STRING</datatype>
      <showtype>HIDDEN</showtype>
      <length>32</length>
      <precision>0</precision>
      <isnotnull>Y</isnotnull>
    </model.grid.ColumnModel>
    <model.grid.ColumnModel>
      <code>groupcode</code>
      <title>用户组号</title>
      <width>64</width>
      <align>center</align>
      <datatype>STRING</datatype>
      <showtype>HIDDEN</showtype>
      <length>32</length>
      <precision>0</precision>
      <isnotnull>Y</isnotnull>
    </model.grid.ColumnModel>
    <model.grid.ColumnModel>
      <code>usercode</code>
      <title>用户号</title>
      <width>128</width>
      <align>center</align>
      <datatype>STRING</datatype>
      <showtype>TEXT</showtype>
      <length>32</length>
      <precision>0</precision>
      <isnotnull>Y</isnotnull>
    </model.grid.ColumnModel>
    <model.grid.ColumnModel>
      <code>username</code>
      <title>用户名</title>
      <width>256</width>
      <align>center</align>
      <showtype>DICTVALUE</showtype>
      <dictid>accoutname</dictid>
      <linkfields>usercode</linkfields>
      <length>64</length>
      <precision>0</precision>
    </model.grid.ColumnModel>
    <model.grid.ColumnModel>
      <code>modifytime</code>
      <title>修改时间</title>
      <width>128</width>
      <align>center</align>
      <datatype>STRING</datatype>
      <showtype>TEXT</showtype>
      <length>19</length>
      <precision>0</precision>
      <isnotnull>N</isnotnull>
    </model.grid.ColumnModel>
  </columns>
  <opts>
    <model.grid.QueryOptModel>
      <id>group</id>
      <title>群组</title>
      <field>groupcode</field>
      <type>LINK</type>
      <option>=</option>
      <required>Y</required>
      <display>HIDDEN</display>
      <dictid>groupcode</dictid>
    </model.grid.QueryOptModel>
  </opts>
  <actions>
    <model.view.ActionModel>
      <code>add</code>
      <name>增加用户</name>
      <type>POPUP</type>
      <showtype>BUTTON</showtype>
      <href>USER</href>
      <display>ICONTEXT</display>
      <rets>users</rets>
      <operand>ROWS</operand>
      <icon>add.gif</icon>
      <target>_self</target>
      <callback>win.addUsersToGroup();</callback>
    </model.view.ActionModel>
    <model.view.ActionModel>
      <code>delete</code>
      <name>删除</name>
      <type>ROWS</type>
      <showtype>BUTTON</showtype>
      <href>deleteusers</href>
      <display>ICONTEXT</display>
      <operand>ROWS</operand>
      <icon>delete.gif</icon>
      <target>_self</target>
    </model.view.ActionModel>
    <model.view.ActionModel>
      <code>exit</code>
      <name>返回</name>
      <type>URL</type>
      <showtype>BUTTON</showtype>
      <url>group.cmd?$ACTION=list</url>
      <display>ICONTEXT</display>
      <operand>FREE</operand>
      <icon>exit.gif</icon>
      <target>_self</target>
    </model.view.ActionModel>
  </actions>
  <query>
    <business>retrieve.eap_usergroup</business>
  </query>
</model.grid.GridModel>