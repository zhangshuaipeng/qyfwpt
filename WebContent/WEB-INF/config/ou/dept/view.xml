      <style>space</style>
      <jsfile>&lt;script&gt;&#xd;
&#xd;
function selectDept() {&#xd;
&#xd;
var selected = document.getElementsByName(&quot;selected&quot;);&#xd;
if (!selected) {&#xd;
    top.window.getDialog(window).close();&#xd;
    return;&#xd;
}&#xd;
var ret = &quot;&quot;;&#xd;
var key = &quot;&quot;;&#xd;
var value = &quot;&quot;;&#xd;
var v;&#xd;
for (var i=0;i&lt;selected.length;i++) {&#xd;
   if (selected[i].checked) {&#xd;
      if (ret!=&quot;&quot;) {&#xd;
          ret += &quot;,&quot;;&#xd;
          key +=&quot;,&quot;;&#xd;
          value += &quot;,&quot;;&#xd;
       }&#xd;
      v = selected[i].value;&#xd;
      ret += v;&#xd;
      key += v.substring(0,v.indexOf(&quot;.&quot;));&#xd;
      value += v.substring(v.indexOf(&quot;.&quot;) + 1);&#xd;
   }&#xd;
&#xd;
}&#xd;
&#xd;
if (ret==&quot;&quot;) {&#xd;
     alert(&quot;请选择部门！&quot;);&#xd;
     return;&#xd;
}&#xd;
&#xd;
top.window.getDialog(window).submit(key + &quot;|&quot; + value);&#xd;
&#xd;
}&#xd;
&#xd;
&lt;/script&gt;</jsfile>
    </model.view.TempletModel>
